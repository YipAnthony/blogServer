<div id="navbar" class="navbar navbar-expand-lg navbar-dark bg-primary">
  <a href="../" class="navbar-brand">
    <img src="/images/arrow-left.svg" class="filterWhite" alt="">
  </a>
  <h1 class="navbar-brand"><%= title %></h1>
</div>
<% errors.forEach(error => { %>
    <p><%= error %> </p>
<% }) %> 
<% if (msg != "") { %>
    <div class="alert alert-warning alert-dismissible fade show" role="alert">
      <%= msg %> 
      <button type="button" class="close" data-dismiss="alert" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
    </div>
<% } %> 
<% if (errorMsg != "") { %>
  <div class="alert alert-warning alert-dismissible fade show" role="alert">
    <%= errorMsg %> 
    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
      <span aria-hidden="true">&times;</span>
    </button>
  </div>
<% } %> 
<form action="" method="post" autocomplete="off">
  <fieldset>
    <div class="form-group">
      <label for="username">Username</label>
      <input id="username" class="form-control" name="username" type="text"> <br>
    </div>
    <div class="form-group">
      <label for="password">Password</label>
      <input id="password" class="form-control" name="password" type="password"> <br>
    </div>
  </fieldset>
    <button class="btn btn-primary" type="submit">Log in</button>
</form>