<% include ../partials/topBar %>

<form action="" method="post" autocomplete="off">
    <fieldset>
        <div class="form-group">
            <label for="title">Title</label>
            <input id="title" class="form-control" name="title" type="text"> <br>

        </div>
        <label for="content">Content</label>
        <textarea id="content" name="content"></textarea>
         <br>
    </fieldset>
    <button name="published" value="false" class="btn btn-warning" type="submit">Save as unpublished</button>
    <button name="published" value="true" class="btn btn-primary" type="submit">Publish</button>
</form>